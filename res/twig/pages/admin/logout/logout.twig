{% extends "base.twig" %}
{% block content %}
    {% if is_logged %}
        <div class="page-header">
            <h1>Sesión cerrada correctamente</h1>
        </div>
        <div class="alert alert-success" role="alert">
            <p>
                <span class="glyphicon glyphicon-ok" aria-hidden="true"></span>
                Tu sesión <strong>AddDate</strong> se cerró correctamente, puedes entrar a la pagina <a href="/home">aqui</a>
            <p/>
        </div>
    {% else %}
        <div class="page-header">
            <h1>Error al cerrar sesión</h1>
        </div>
        <div class="alert alert-warning" role="alert">
            <p>
                <span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span>
                No puedes cerrar sesión debido a que no la habías iniciado anteriormente
            <p/>
        </div>
    {% endif %}
{% endblock %}